<style type="text/css">
    .whirl.shadow::after {
        animation: none;
        z-index: 13;
    }
    .whirl.shadow::before {
        z-index: 13;
    }

    .btn-back {
        color: #5e5e5e;
        cursor: pointer;
        font-family: yekan;
        font-size: 20px;

        text-decoration: none;
        -moz-transition: background-color 300ms ease-out;
        -webkit-transition: background-color 300ms ease-out;
        -o-transition: background-color 300ms ease-out;
        transition: background-color 300ms ease-out;
    }
        .btn-back .icon-action-undo {
            font-size: 25px;
            position: relative;
            top: 3px;
        }
        .btn-back:hover {
            color: #FF7200;
            text-decoration: none;
            -moz-transition: background-color 300ms ease-in;
            -webkit-transition: background-color 300ms ease-in;
            -o-transition: background-color 300ms ease-in;
            transition: background-color 300ms ease-in;
        }

    .panel-order-details {
    }

    .panel-last-designs {
    }
        .panel-last-designs ul {
            margin: 0;
            padding: 0;
            list-style: none;
            text-align: right;
        }
            .panel-last-designs ul li {
                background-color: #f6f6f6;
                border: 1px solid #bebebe;
                display: inline-block;
                height: 90px;
                margin: 5px;
                width: 90px;
            }
                .panel-last-designs ul li a {
                    display: block;
                    height: 88px;
                    width: 88px;
                }

        #checkoutDesign ul {
            border-top: 1px solid #bebebe;
            direction: ltr;
            margin: 15px 0 0 0;
            padding: 10px 0 0 0;
            list-style: none;
            text-align: left;
        }
            #checkoutDesign ul li {
                background-color: #f6f6f6;
                border: 1px solid #bebebe;
                cursor: pointer;
                display: inline-block;
                height: 90px;
                margin: 5px;
                width: 90px;
            }

    .pre {
        background-color: transparent;
        border: none;
        color: #5e5e5e;
        display: inline;
        font-family: BNazanin;
        font-size: 15.5px;
        font-weight: normal;
        margin: 0;
        padding: 0;
        white-space: pre-wrap;
        -moz-border-radius: 0;
        -webkit-border-radius: 0;
        border-radius: 0;
    }
    .pre2 {
        background-color: transparent;
        border: none;
        color: #5e5e5e;
        display: inline;
        font-family: yekan;
        font-size: 14px;
        font-weight: normal;
        margin: 0;
        padding: 0;
        white-space: pre-wrap;
        -moz-border-radius: 0;
        -webkit-border-radius: 0;
        border-radius: 0;
    }

    .image-cropper {
        direction: ltr;
        margin: 0 auto;
        position: relative;
        text-align: left;
        width: 490px;
    }
    .cropit-image-preview {
        background-color: #f6f6f6;
        background-size: cover;
        border: 1px solid #bebebe;
        border-radius: 3px;
        margin: 7px auto;
        width: 490px;
        height: 354px;
        cursor: move;
        position:relative;
    }
    .cropit-image-background {
        opacity: .2;
        cursor: auto;
    }
    .image-size-label {
        margin-top: 10px;
    }
    .cropit-image-zoom-input {
        -moz-appearance: none;
        background: #eee none repeat scroll 0 0;
        border-radius: 5px;
        box-sizing: border-box;
        color: inherit;
        height: 5px;
        margin: 10px 0;
        line-height: normal;
        outline: medium none;
        position: relative;
        vertical-align: middle;
    }

    /* Custom, iPhone Retina */
	@@media only screen and (min-width : 320px) {
        .image-cropper {
            width: 260px;
        }
        .cropit-image-preview {
            width: 260px;
        }
	}
	/* Extra Small Devices, Phones */
	@@media only screen and (min-width : 480px) {
        .image-cropper {
            width: 400px;
        }
        .cropit-image-preview {
            width: 400px;
        }
	}
	/* Small Devices, Tablets */
	@@media only screen and (min-width : 768px) {
        .image-cropper {
            width: 490px;
        }
        .cropit-image-preview {
            width: 490px;
        }
	}
	/* Medium Devices, Desktops */
	@@media only screen and (min-width : 992px){
        .image-cropper {
            width: 490px;
        }
        .cropit-image-preview {
            width: 490px;
        }
	}
	/* Large Devices, Wide Screens */
	@@media only screen and (min-width : 1200px){
		.image-cropper {
            width: 490px;
        }
        .cropit-image-preview {
            width: 490px;
        }
	}
</style>

<div class="container-fluid">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="clearfix" style="margin-bottom: 15px;">
                <div class="pull-left">
                    <a class="btn-back" ng-click="back()">
                        بازگشت
                        <i class="icon-action-undo fa-2x"></i>
                    </a>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="panel panel-default panel-order-details"
             ng-class="{'whirl': fetchDataLoading , 'shadow': fetchDataLoading}">
            <div class="panel-heading">
                <div class="panel-title">
                    <i></i>
                    مشخصات سفارش
                </div>
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                        <div class="form-group">
                            <span>شرح سفارش :</span>
                            &nbsp;
                            <span class="text-muted">{{order.Form.Title}}</span>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12">
                        <div class="form-group">
                            <span>کد سفارش :</span>
                            &nbsp;
                            <span class="text-muted"
                                  style="font-family:BNazanin;font-size:16px;">{{order.Code}}</span>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12">
                        <div class="form-group">
                            <span>تاریخ سفارش :</span>
                            &nbsp;
                            <span class="text-muted"
                                  style="direction:ltr;font-family:BNazanin;font-size:16px;" ">{{order.Time + ' - ' + order.PersianRegisterDate}}</span>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                        <div class="form-group">
                            <div class="text-info" style="cursor:pointer;display:inline-block;"
                                 ng-click="showDetails()">
                                <span>نمایش اطلاعات سفارش</span>
                                <span class="small">[کلیک کنید]</span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="panel panel-default panel-last-designs"
             ng-class="{'whirl': fetchDesignLoading , 'shadow': fetchDesignLoading}">
            <div class="panel-heading">
                <div class="panel-title">
                    <i></i>
                   فایل های طراحی شده
                </div>
            </div>
            <div class="panel-body"
                 style="min-height:250px;">
                <div ng-repeat="design in designs">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="form-group">
                                <span>تاریخ ارسال :</span>
                                &nbsp;
                                <span class="text-muted"
                                      style="direction:ltr;font-family:BNazanin;font-size:16px;">{{design.Time + ' - ' + design.PersianRegisterDate}}</span>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-121 col-md-12 col-sm-12 col-xs-12">
                            <div class="form-group">
                                <pre class="pre2">{{'توضیحات :' + design.Description}}</pre>
                            </div>
                        </div>
                    </div>
                    <ul>
                        <li ng-repeat="file in design.Files">
                            <img src="{{file.PicturePath}}" imagescale="" />
                            <img src="/Images/ReviewIcon/accept.png"
                                 style="display:block;height:22px;width:22px;position:absolute;bottom:2px;right:2px;"
                                 ng-if="file.State == states.Accept" />
                            <img src="/Images/ReviewIcon/maybe.png"
                                 style="display:block;height:22px;width:22px;position:absolute;bottom:2px;right:2px;"
                                 ng-if="file.State == states.Maybe" />
                            <img src="/Images/ReviewIcon/unaccept.png"
                                 style="display:block;height:22px;width:22px;position:absolute;bottom:2px;right:2px;"
                                 ng-if="file.State == states.UnAccept" />
                        </li>
                    </ul>
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="clearfix">
                                <div class="pull-left">
                                    <button class="btn btn-labeled btn-warning" type="button"
                                            ng-click="showCheckouDesigntModal($index)"
                                            ng-if="design.IsReview == false && $first">
                                        <span class="btn-label btn-label-right">
                                            <i class="fa fa-check"></i>
                                        </span>
                                        بررسی
                                    </button>
                                    <button class="btn btn-labeled btn-info" type="button"
                                            ng-click="showDesignModal($index)"
                                            ng-if="design.IsReview == true">
                                        <span class="btn-label btn-label-right">
                                            <i class="fa fa-check"></i>
                                        </span>
                                        نمایش بررسی
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <hr class="hr1" style="margin:15px 0;" ng-hide="$last" />
                </div>
            </div>
        </div>
    </div>
</div>

<!-- Start Order Details Modal -->
<script type="text/ng-template" id="/ShowDetailsContent.html">
    <div class="modal-header">
        <div class="modal-title">
            <i></i>
            نمایش سفارش
        </div>
    </div>
    <div class="modal-body" ng-class="{'whirl': fetchLoading , 'shadow': fetchLoading}">
        <div class="row">
            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                <div class="form-group">
                    <span>شرح سفارش :</span>
                    &nbsp;
                    <span class="text-muted">{{order.Form.Title}}</span>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12">
                <div class="form-group">
                    <span>کد سفارش :</span>
                    &nbsp;
                    <span class="text-muted"
                          style="font-family:BNazanin;font-size:16px;">{{order.Code}}</span>
                </div>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12">
                <div class="form-group">
                    <span>تاریخ سفارش :</span>
                    &nbsp;
                    <span class="text-muted"
                          style="direction:ltr;font-family:BNazanin;font-size:16px;" ">{{order.Time + ' - ' + order.PersianRegisterDate}}</span>
                </div>
            </div>
        </div>

        <hr class="hr1" />

        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="table-responsive"
                     style="min-height:300px;">
                    <table class="table table-striped">
                        <tbody>
                            <tr ng-repeat="item in values">
                                <td style="min-width:45px;width:45px;">
                                    <span style="margin-right:4px;">{{$index + 1}}</span>
                                </td>
                                <td style="min-width:200px;width:200px;">{{item.Name}}</td>
                                <td style="min-width:300px;" ng-if="item.Type == 0 || item.Type == 2 || item.Type == 8 || item.Type == 9">{{item.Value}}</td>
                                <td style="min-width:300px;" ng-if="item.Type == 1"><pre class="pre">{{item.Value}}</pre></td>
                                <td style="min-width:300px;" ng-if="item.Type == 3">
                                    <span style="font-family:Tahoma;font-size:13px;">{{item.Value}} </span>
                                    <i style="display:inline-block;height:16px;width:16px;margin-right:2px;position:relative;top:4px;background-color:{{item.Value}};"></i>
                                </td>
                                <td style="min-width:300px;" ng-if="item.Type == 4">
                                    <a href="{{item.FilePath}}" target="_blank">{{item.Name}}</a>
                                </td>
                                <td style="min-width:300px;" ng-if="item.Type == 5">
                                    <span ng-if="item.Value == true">دارد</span>
                                    <span ng-if="item.Value == false">ندارد</span>
                                </td>
                                <td style="min-width:300px;" ng-if="item.Type == 6">
                                    <a href="{{item.Value}}" target="_blank"
                                       style="font-family:Tahoma;font-size:13px;">{{item.Value}}</a>
                                </td>
                                <td ng-if="item.Type == 7">
                                    <span style="font-family:BNazanin;font-size:16px;">{{item.Value}}</span>
                                </td>
                                <td ng-if="item.Type == 10">
                                    <div ng-repeat="value in item.Values">{{($index + 1) + '- ' + value}}</div>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <div class="modal-footer">
        <div class="clearfix">
            <div class="pull-left">
                <button class="btn btn-labeled btn-default"
                        ng-click="close()">
                    <span class=" btn-label btn-label-right">
                        <i class="fa fa-times"></i>
                    </span>
                    خروج
                </button>
            </div>
        </div>
    </div>
</script>
<!-- End Order Details Modal -->

<!-- Start Checkout Designs Modal -->
<script type="text/ng-template" id="/CheckoutDesignContent.html">
    <div class="modal-header">
        <div class="modal-title">
            <i></i>
            بررسی فایل های طراحی شده
        </div>
    </div>
    <div id="checkoutDesign" class="modal-body" ng-class="{'whirl': fetchLoading , 'shadow': fetchLoading}">
        <div class="row">
            <div class="col-lg-5 col-md-5 col-sm-12 col-xs-12 hidden-sm hidden-xs">
                <button class="btn btn-block btn-labeled"
                        type="button" style="text-align:right;min-height:30px;"
                        ng-class="{'btn-default': selectedFile.State != states.Accept, 'btn-success': selectedFile.State == states.Accept}"
                        ng-click="selectAcceptedFile()"
                        ng-show="acceptedFile == null || acceptedFile.Id == selectedFile.Id">
                    <span class="btn-label btn-label-right hidden-sm hidden-xs">
                        <i class="fa fa-check"></i>
                    </span>
                    این طرح به طور کامل مورد تأیید می باشد
                </button>
                <button class="btn btn-block btn-labeled btn-default"
                        type="button" style="text-align:right;min-height:30px;"
                        ng-class="{'btn-default': selectedFile.State != states.Maybe, 'btn-warning': selectedFile.State == states.Maybe}"
                        ng-click="selectMaybeFile()">
                    <span class="btn-label btn-label-right hidden-sm hidden-xs">
                        <i class="fa fa-check"></i>
                    </span>
                    این طرح مورد تأیید است ولی نیاز به اصلاح دارد
                </button>
                <button class="btn btn-block btn-labeled btn-default"
                        type="button" style="text-align:right;min-height:30px;"
                        ng-class="{'btn-default': selectedFile.State != states.UnAccept, 'btn-danger': selectedFile.State == states.UnAccept}"
                        ng-click="selectUnAcceptedFile()">
                    <span class="btn-label btn-label-right hidden-sm hidden-xs">
                        <i class="fa fa-times"></i>
                    </span>
                    این طرح مورد تأیید نمی باشد
                </button>
                <div class="form-group">
                    <label class="control-label">توضیحات</label>
                    <textarea class="form-control"
                              style="height:120px;resize:none;overflow:hidden;"
                              ng-model="selectedFile.CustomerDescription"></textarea>
                </div>
            </div>
            <div class="col-lg-7 col-md-7 col-sm-12 col-xs-12">
                <div class="image-cropper" cropit="" image="selectedFile" image-src="{{selectedFile.PicturePath}}">
                    <div class="cropit-image-preview"></div>
                    <input type="range" class="cropit-image-zoom-input" />
                    <img src="/Images/UI/Pagination/btn-next-2.png"
                         style="display:block;height:26px;width:26px;position:absolute;top:164px;right:5px;cursor:pointer;"
                         ng-show="design.Files.length > 1" ng-click="selectNextFile()" />
                    <img src="/Images/UI/Pagination/btn-back-2.png"
                         style="display:block;height:26px;width:26px;position:absolute;top:164px;left:5px;cursor:pointer;"
                         ng-show="design.Files.length > 1" ng-click="selectPrevFile()" />
                </div>
            </div>
            <div class="col-lg-5 col-md-5 col-sm-12 col-xs-12 visible-sm visible-xs">
                <button class="btn btn-block btn-labeled"
                        type="button" style="text-align:right;min-height:30px;"
                        ng-class="{'btn-default': selectedFile.State != states.Accept, 'btn-success': selectedFile.State == states.Accept}"
                        ng-click="selectAcceptedFile()"
                        ng-show="acceptedFile == null || acceptedFile.Id == selectedFile.Id">
                    <span class="btn-label btn-label-right hidden-sm hidden-xs">
                        <i class="fa fa-check"></i>
                    </span>
                    این طرح به طور کامل مورد تأیید می باشد
                </button>
                <button class="btn btn-block btn-labeled btn-default"
                        type="button" style="text-align:right;min-height:30px;"
                        ng-class="{'btn-default': selectedFile.State != states.Maybe, 'btn-warning': selectedFile.State == states.Maybe}"
                        ng-click="selectMaybeFile()">
                    <span class="btn-label btn-label-right hidden-sm hidden-xs">
                        <i class="fa fa-check"></i>
                    </span>
                    این طرح مورد تأیید است ولی نیاز به اصلاح دارد
                </button>
                <button class="btn btn-block btn-labeled btn-default"
                        type="button" style="text-align:right;min-height:30px;"
                        ng-class="{'btn-default': selectedFile.State != states.UnAccept, 'btn-danger': selectedFile.State == states.UnAccept}"
                        ng-click="selectUnAcceptedFile()">
                    <span class="btn-label btn-label-right hidden-sm hidden-xs">
                        <i class="fa fa-times"></i>
                    </span>
                    این طرح مورد تأیید نمی باشد
                </button>
                <div class="form-group">
                    <label class="control-label">توضیحات</label>
                    <textarea class="form-control"
                              style="height:120px;resize:none;overflow:hidden;"
                              ng-model="selectedFile.CustomerDescription"></textarea>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 hidden-sm hidden-xs">
                <ul>
                    <li ng-repeat="file in design.Files" ng-click="selectFile($index)">
                        <img src="{{file.PicturePath}}" imagescale="" />
                        <img src="/Images/ReviewIcon/accept.png"
                             style="display:block;height:22px;width:22px;position:absolute;bottom:2px;right:2px;"
                             ng-if="file.State == states.Accept" />
                        <img src="/Images/ReviewIcon/maybe.png"
                             style="display:block;height:22px;width:22px;position:absolute;bottom:2px;right:2px;"
                             ng-if="file.State == states.Maybe" />
                        <img src="/Images/ReviewIcon/unaccept.png"
                             style="display:block;height:22px;width:22px;position:absolute;bottom:2px;right:2px;"
                             ng-if="file.State == states.UnAccept" />
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <div class="modal-footer">
        <div class="clearfix">
            <div class="pull-left">
                <button class="btn btn-labeled btn-default"
                        ng-click="close()">
                    <span class=" btn-label btn-label-right">
                        <i class="fa fa-times"></i>
                    </span>
                    خروج
                </button>
                <button class="btn btn-labeled btn-warning"
                        ng-click="sendReview()">
                    <span class=" btn-label btn-label-right">
                        <i class="fa fa-check"></i>
                    </span>
                    ارسال نظرات شما
                </button>
            </div>
        </div>
    </div>
</script>
<!-- End Checkout Designs Modal -->

<!-- Start Designs Modal -->
