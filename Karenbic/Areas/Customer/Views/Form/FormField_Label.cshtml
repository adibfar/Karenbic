<div class="container-fluid" ng-if="item.type == 12">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="form-group">
                <div ng-if="item.data.upline == true" style="border-bottom:1px solid #808080;height:1px;"></div>
                <label style="cursor:pointer;color:{{item.data.color}};font-family:{{item.data.font.Title}};font-size:{{item.data.size}}px;"
                       ng-if="item.data.hasPictureHelpFile == true"
                       tooltipster="" tooltipster-title="<img src='{{item.data.pictureHelpPath}}' />">{{item.data.title}}</label>
                <label style="color:{{item.data.color}};font-family:{{item.data.font.Title}};font-size:{{item.data.size}}px;"
                       ng-if="item.data.hasPictureHelpFile == false">{{item.data.title}}</label>
                <div ng-if="item.data.underline == true" style="border-bottom:1px solid #808080;height:1px;"></div>
            </div>
        </div>
    </div>
</div>